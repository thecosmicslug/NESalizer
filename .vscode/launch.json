    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "nesalizer",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "targetArchitecture": "arm",
            "setupCommands":
            [
            {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
            }
            ],
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "miDebuggerServerAddress": "192.168.0.15:2346",
            "miDebuggerArgs": "",
            },
            
    ]
}
